PROGRAM IS
    a
IN
    READ a;
    IF a < 5 THEN
        WRITE 1;
    ELSE
        WRITE 0;
    ENDIF
    IF a > 5 THEN
        WRITE 1;
    ELSE
        WRITE 0;
    ENDIF
    IF a >= 5 THEN
        WRITE 1;
    ELSE
        WRITE 0;
    ENDIF
    IF a <= 5 THEN
        WRITE 1;
    ELSE
        WRITE 0;
    ENDIF
    IF a = 5 THEN
        WRITE 1;
    ELSE
        WRITE 0;
    ENDIF
    IF a != 5 THEN
        WRITE 1;
    ELSE
        WRITE 0;
    ENDIF
END